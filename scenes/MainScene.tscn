script/source = "extends Spatial

onready var camera = get_node(\"Camera\")
onready var piehud = get_node(\"CanvasLayer/PieHUD\")

func _input(event):
    piehud.visible = Input.is_action_pressed(\"activate_pie\")

func _on_PieHUD_released(position, slice, tier):
    var world_position = camera.project_ray_origin(position)
#    get_node(\"CanvasLayer/Label\").text = \"%s,%s\" % [position.x, position.y]
    get_node(\"CanvasLayer/Label\").text = \" %s,%s %s %s\" % [world_position.x, world_position.y, slice, tier]"

[sub_resource type="GDScript" id=2]

script/source = "extends DirectionalLight

const SPEED = 10
onready var env = get_node(\"WorldEnvironment\")

func _process(delta):
    rotate_x(delta / 10)

    if rotation_degrees.x < 0:
        light_energy = range_lerp(rotation_degrees.x, 0, -180, 0.0, 1.0)
        env.environment.background_energy = range_lerp(rotation_degrees.x, 0, -180, 0.1, 1.0)
    else:
        light_energy = 0
        env.environment.background_energy = 0.1"

